CREATE OR REPLACE PROCEDURE CONSINCO.NAGP_GERA_CARGA_PRECO_ECOMM AS

  vsHora         VARCHAR2(2);
  vsMin          VARCHAR2(2);

BEGIN

  SELECT TO_CHAR(SYSDATE, 'HH24'), TO_CHAR(SYSDATE, 'MI')
    INTO vsHora, vsMin
    FROM DUAL;

  IF vsHora IN ('01', '09', '13') OR vsHora = '18' AND vsMin = '30'

    THEN

    BEGIN
      CONSINCO.SP_CARGA_PRECO_NAG_ECOMMERCE;
    END;

  END IF;

  COMMIT;

END;
